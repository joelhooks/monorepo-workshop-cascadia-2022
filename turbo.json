{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**"]
    },
    "build#marketing-site": {
      "dependsOn": ["^build", "$VERCEL_URL", "$PORT", "$NODE_ENV"],
      "outputs": [".next/**", "dist/**"]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test": {
      "outputs": [],
      "dependsOn": []
    },
    "test:e2e": {
      "cache": false,
      "dependsOn": []
    },
    "test:e2e#landing-page": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
